{
  "name": "@foundriesio/log",
  "private": false,
  "version": "1.0.0",
  "engines": {
    "node": ">= 12"
  },
  "description": "A pino-based logger for *.foundries.io web applications",
  "main": "dist/main.js",
  "files": [
    "dist/*.js",
    "LICENSE.md",
    "package-lock.json"
  ],
  "scripts": {
