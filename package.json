  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-security": "^1.4.0",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/foundriesio/fio-log.git"
  },
  "bugs": {
    "url": "https://github.com/foundriesio/fio-log/issues"
